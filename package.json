{
  "name": "a10-widget-lib",
  "version": "0.0.1",
  "description": "A10 Widgets With A10 Business",
  "main": "./dist/index.js",
  "scripts": {
    "build": "better-npm-run build",
    "clean": "rimraf dist lib",
    "prepublish": "npm run clean && npm run build",
    "lint": "eslint --fix -c .eslintrc client",
    "test": "better-npm-run test"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --config webpack.config.js ",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "mocha-webpack --colors  --webpack-config webpack.config.test.js \"client/**/__tests__/*.spec.js\"",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "keywords": [
    "A10Widget",
    "WidgetLIb"
  ],
  "author": "zli",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2015-native-modules": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "better-npm-run": "^0.0.14",
    "clean-webpack-plugin": "^0.1.15",
    "es6-promise-promise": "^1.0.0",
    "eslint": "^3.16.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "transform-runtime": "^0.0.0",
    "webpack": "^2.2.1"
  },
  "peerDependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
}
